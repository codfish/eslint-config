{
  "name": "eslint-config-codfish",
  "version": "0.0.0-semantically-released",
  "description": "ESLint configuration extending airbnb & prettier.",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"**/*.{json,css,scss,md}\" && npm run lint -- --fix",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codfish/eslint-config-codfish.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "airbnb",
    "codfish",
    "prettier",
    "javascript",
    "styleguide"
  ],
  "author": "Chris O'Donnell <dev@codfish.io> (http://codfish.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codfish/eslint-config-codfish/issues"
  },
  "homepage": "https://github.com/codfish/eslint-config-codfish#readme",
  "peerDependencies": {
    "eslint": "^7.5.0",
    "prettier": "^2.0.0"
  },
  "engines": {
    "node": "^10.12.0 || >=12.0.0",
    "npm": ">= 6",
    "yarn": ">= 1"
  },
  "dependencies": {
    "arrify": "^2.0.1",
    "babel-eslint": "^10.0.3",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-kentcdodds": "^17.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "lodash.has": "^4.5.2",
    "read-pkg-up": "^7.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "eslint": "^7.15.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "extends": [
      "./index.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  }
}
