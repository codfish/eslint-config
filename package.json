{
  "name": "eslint-config-codfish",
  "version": "0.0.0-semantically-released",
  "description": "ESLint configuration extending airbnb & prettier.",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"**/*.{json,css,scss,md}\" && npm run lint -- --fix",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codfish/eslint-config-codfish.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "airbnb",
    "codfish",
    "prettier",
    "javascript",
    "styleguide"
  ],
  "author": "Chris O'Donnell <dev@codfish.io> (http://codfish.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codfish/eslint-config-codfish/issues"
  },
  "homepage": "https://github.com/codfish/eslint-config-codfish#readme",
  "peerDependencies": {
    "eslint": ">= 5",
    "prettier": ">= 1"
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 6",
    "yarn": ">= 1"
  },
  "dependencies": {
    "arrify": "^2.0.1",
    "babel-eslint": "^10.0.3",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-kentcdodds": "^14.4.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.16.0",
    "lodash.has": "^4.5.2",
    "read-pkg-up": "^6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "eslint": "^6.4.0",
    "husky": "^3.0.5",
    "lint-staged": "^10.1.6",
    "prettier": "^1.18.2"
  },
  "eslintConfig": {
    "extends": [
      "./index.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  }
}
